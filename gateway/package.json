{
  "name": "gateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev -r dotenv/config src/server.ts",
    "start:withoutregistry": "ts-node-dev -r dotenv/config src/serverWithoutSchemaRegistry.ts",
    "lint": "eslint */**/*.{js,ts} --quiet --fix",
    "preservice:push": "ts-node-dev -r dotenv/config src/scripts/generateFederatedSchema",
    "service:push": "ts-node-dev -r dotenv/config src/scripts/pushSchema",
    "service:push:remote": "ts-node-dev -r dotenv/config src/scripts/pushRemoteSchema",
    "service:list": "apollo service:list --variant=$variant",
    "service:delete": "apollo service:delete --variant=$variant --serviceName=$serviceName"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/gateway": "^0.26.1",
    "apollo-server-express": "^2.24.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-tools/merge": "^6.2.13",
    "@ljharb/eslint-config": "^17.5.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "apollo": "^2.32.10",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-env": "^19.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "graphql-request": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
